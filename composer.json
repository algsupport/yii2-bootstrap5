{
    "name": "algsupport/yii2-bootstrap5",
    "description": "The Twitter Bootstrap v5 extension for the Yii framework",
    "keywords": [
        "yii2",
        "bootstrap",
        "bootstrap5"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/algsupport/yii2-bootstrap5"
    },
    "authors": [
        {
            "name": "Albert Gukasian",
            "email": "admin@algsupport.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "algsupport/yii2": "*",
        "bower-asset/bootstrap": "^5.1.0"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\bootstrap5\\": "src"
        }
    },
    "extra": {
        "bootstrap": "yii\\bootstrap5\\i18n\\TranslationBootstrap"
    }
}
